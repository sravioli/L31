// Example markdownlint configuration with all properties set to their default value
{
  // Default state for all rules
  "default": true,


  // MD013/line-length - Line length
  "MD013": {
    // Include tables
    "tables": false,
  },

  // MD046/code-block-style - Code block style
  "MD046": {
    // Block style
    "style": "fenced"
  },
}
